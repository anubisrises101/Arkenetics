<div id="description">
    <p>SUP </p>
</div>
<div id="logoBox">
    <img src="" alt="" id="logo">
</div>

<% if (userCreatures.length) { %>
    <ul>
        <% userCreatures.forEach((creature) => { %>
        <li>
            <a href=""> <%= creature.name %> </a>
        </li>
        <% }) %>
    </ul>
<% } %>

<% if (allCreatures.length) { %>
    <select name="name"></select>
      <% allCreatures.forEach((creature) => { %>
        <option value="<%= creature.name %>"><%= creature.name %></option>
      <% }) %>
  <% } else { %>
    <h1>IT ISNT WORKING YOU DOLT</h1>
  <% } %>

